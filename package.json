{
	"name": "ui-builder-dock-for-servicenow",
	"displayName": "UI Builder Dock for ServiceNow",
	"version": "1.0.10",
	"description": "A lightweight utility that enhances ServiceNow UI Builder development by making key parts easier to explore and understand.",
	"author": "Jan Moser <m.janmoser@gmail.com>",
	"homepage": "https://myuibcorner.com",
	"scripts": {
		"dev": "plasmo dev",
		"build": "plasmo build",
		"package": "plasmo package"
	},
	"dependencies": {
		"@ant-design/icons": "~5.6.1",
		"@plasmohq/storage": "^1.15.0",
		"antd": "^5.24.5",
		"plasmo": "0.90.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-error-boundary": "^6.0.0"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "4.1.1",
		"@types/chrome": "0.0.258",
		"@types/node": "20.11.5",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"prettier": "3.2.4",
		"typescript": "5.3.3"
	},
	"manifest": {
		"host_permissions": [
			"https://*.service-now.com/*"
		],
		"web_accessible_resources": [
			{
				"resources": [
					"inject.js"
				],
				"matches": [
					"https://*.service-now.com/*"
				]
			}
		]
	}
}